			<xsd:element ref="cbc:ChargeIndicator"/>
			<xsd:element name="ReasonCode" type="rsn:AllowanceChargeReasonCodeType" minOccurs="0"/>
			<xsd:element ref="cbc:MultiplierFactorNumeric" minOccurs="0"/>
