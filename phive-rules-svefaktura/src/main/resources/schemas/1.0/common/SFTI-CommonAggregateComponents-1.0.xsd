			<xsd:element name="PayeePartyName" type="SFTIPartyNameType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SFTIPaymentTermsType">
		<xsd:sequence>
			<xsd:element ref="cbc:Note" minOccurs="0"/>
