		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SFTITaxTotalType">
		<xsd:sequence>
			<xsd:element ref="cbc:TotalTaxAmount"/>
			<xsd:element ref="TaxSubTotal" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
