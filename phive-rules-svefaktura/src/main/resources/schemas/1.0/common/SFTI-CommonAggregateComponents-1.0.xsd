			<xsd:element ref="BasePrice" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SFTIItemIdentificationType">
		<xsd:sequence>
			<xsd:element name="ID" type="udt:IdentifierType"/>
		</xsd:sequence>
